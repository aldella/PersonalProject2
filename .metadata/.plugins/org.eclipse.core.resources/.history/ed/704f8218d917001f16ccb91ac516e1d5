package com.personalproject2.per.dto;

public class Member {
	
	String id;
	String pw;
	String name;
	String email;
	String tel;
	String addr1;
	String addr2;
	String postcode;
	String birth;
	int point;
	String regdate;
	
	/*
	 CREATE TABLE member(
	 id varchar(20) primary key, 
	 pw varchar(500) not null, 
	 name varchar(100) not null, 
	 email varchar(200) not null, 
	 tel varchar(100) not null, 
	 addr1 varchar(200), 
	 addr2 varchar(100), 
	 postcode varchar(20), 
	 birth date, 
	 point int , 
	 regdate timestamp default current_timestamp);

	 */
}
